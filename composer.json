{
  "name": "deeem/hexlet-psr-linter",
  "type": "library",
  "bin": ["bin/psr-linter"],
  "require": {
    "nikic/php-parser": "*",
    "nategood/commando": "*",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "codeclimate/php-test-reporter": "dev-master",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "PsrLinter\\": "src/"
    },
    "files": [
      "src/makeLinter.php",
      "src/lintCli.php",
      "src/report.php"
    ]
  }
}
